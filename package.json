    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-plugin-solid": "^0.12.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "taze": "^0.11.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.1",
    "vitest": "^0.33.0"
